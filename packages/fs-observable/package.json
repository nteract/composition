{
  "name": "fs-observable",
  "version": "3.0.2",
  "description": "Observables for node fs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "nteractDesktop": "src/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run:build",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "tsc -b --clean",

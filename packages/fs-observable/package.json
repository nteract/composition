  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.1"
  },
  "peerDependencies": {
    "rxjs": "^6.3.3"
  }

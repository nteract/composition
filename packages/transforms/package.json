{
  "name": "@nteract/transforms",
  "version": "1.0.6",
  "description": "Common transforms for Jupyter",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/nteract.git"
  },
  "keywords": [
    "nteract",
    "transforms",
    "notebook"
  ],
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/nteract/issues"
  },
  "homepage": "https://github.com/nteract/nteract#readme",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "setupFiles": [
      "@nteract/mockument"
    ]
  },
  "dependencies": {
    "react-json-tree": "^0.10.1",
    "ansi-to-react": "^1.1.0",
    "mathjax-electron": "^1.2.0",
    "commonmark": "^0.27.0",
    "commonmark-react-renderer": "^4.3.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^15.4.2"
  }
}

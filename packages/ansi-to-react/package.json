  "license": "MPL-2.0",
  "dependencies": {
    "anser": "^1.4.1",
    "escape-carriage": "^1.3.0"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}

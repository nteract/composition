{
  "name": "ansi-to-react",
  "version": "3.3.5",
  "description": "ANSI to React Elements",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "nteractDesktop": "src/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "build": "npm run build:clean && npm run build:lib",

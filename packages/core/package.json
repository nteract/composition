{
  "name": "@nteract/core",
  "version": "0.0.6",
  "description": "core modules and components for nteract apps",
  "main": "lib/index.js",
  "nteractDesktop": "src/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "build:lib:watch": "npm run build:lib -- --watch"
  },
  "dependencies": {
    "@nteract/commutable": "^2.2.0",
    "@nteract/display-area": "^3.1.0",
    "@nteract/editor": "^4.2.1",
    "@nteract/transforms": "^3.0.3",
    "date-fns": "^1.28.5",
    "commonmark": "^0.28.0",
    "commonmark-react-renderer": "^4.3.3",
    "escape-carriage": "^1.2.0",
    "mathjax-electron": "^2.0.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-redux": "^5.0.5",
    "react-simple-dropdown": "^3.0.0",
    "react-syntax-highlighter": "^5.8.0",
    "redux": "^3.7.0",
    "rxjs": "^5.5.0",
    "styled-jsx": "^2.1.3",
    "uuid": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["nteract", "ohgodamonomoduleinamonorepo"],
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause"
}

{
  "name": "@nteract/notebook-app-component",
  "version": "4.0.0-alpha.0",
  "description": "Editable notebook app component, backed by @nteract/core's store",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "nteractDesktop": "src/index.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "tsc -b --clean",
    "build:lib": "tsc -b",
    "build:lib:watch": "tsc -b --watch",
    "build:watch": "npm run build:clean && npm run build:lib:watch"
  },
  "repository": "https://github.com/nteract/nteract/tree/master/packages/notebook-app-component",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nteract/actions": "^1.0.0",
    "@nteract/commutable": "^6.0.0-alpha.0",
    "@nteract/connected-components": "^5.0.0-alpha.0",
    "@nteract/core": "^9.0.0-alpha.0",
    "@nteract/display-area": "^5.0.0-alpha.0",
    "@nteract/dropdown-menu": "^0.5.7",
    "@nteract/editor": "^8.0.0-alpha.0",
    "@nteract/markdown": "^3.0.1",
    "@nteract/messaging": "^5.0.0-alpha.0",
    "@nteract/octicons": "^1.0.0-alpha.0",
    "@nteract/presentational-components": "^2.0.0-alpha.0",
    "@nteract/transforms": "^5.0.0-alpha.0",
    "@nteract/types": "^3.1.0",
    "date-fns": "^1.29.0",
    "react-dnd": "^7.0.0",
    "react-dnd-html5-backend": "^7.0.0",
    "react-hot-loader": "^4.1.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc.12",
    "react": "^16.3.2"

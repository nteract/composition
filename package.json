{
  "name": "composition",
  "version": "0.0.1",
  "description": ":notebook: Electron app of the Jupyter Notebook",
  "main": "main.js",
  "scripts": {
    "lint": "eslint .",
    "test": "electron-mocha --renderer --compilers js:babel-core/register 'test/**/*.@(js|jsx)'",
    "start": "electron main.js",
    "build": "npm run build:osx && npm run build:win32 && npm run build:linux",
    "build:osx": "electron-packager ./ Composition --platform=darwin --arch=x64 --version=0.36.2",
    "build:win32": "electron-packager ./ Composition --platform=win32 --arch=x64 --version=0.36.2",
    "build:linux": "electron-packager ./ Composition --platform=linux --arch=x64 --version=0.36.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nteract/composition.git"
  },
  "keywords": [
    "jupyter",
    "electron",
    "notebook",
    "nteract",
    "data"
  ],
  "author": "rgbkrk",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/composition/issues"
  },
  "homepage": "https://github.com/nteract/composition",
  "dependencies": {
    "ansi-to-html": "^0.3.0",
    "codemirror": "^5.10.0",
    "color": "^0.10.1",
    "commutable": "^0.1.2",
    "enchannel-zmq-backend": "^0.4.0",
    "history": "^1.17.0",
    "ijavascript": "^5.0.8",
    "immutable": "^3.7.6",
    "normalize.css": "^3.0.3",
    "react": "^0.14.5",
    "react-code-mirror": "^3.0.6",
    "react-dom": "^0.14.5",
    "react-jupyter-display-area": "0.0.1",
    "react-markdown": "^1.2.1",
    "react-router": "^1.0.3",
    "rx": "^4.0.7",
    "source-code-pro": "git://github.com/adobe-fonts/source-code-pro.git#release",
    "source-sans-pro": "git://github.com/adobe-fonts/source-sans-pro.git#release",
    "spawnteract": "0.0.4",
    "transformime-react": "0.3.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "electron-compile": "^2.0.4",
    "electron-compilers": "^2.0.4",
    "electron-mocha": "^0.7.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.2",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "react-addons-test-utils": "^0.14.5"
  }
}

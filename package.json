{
  "name": "nteract",
  "version": "0.0.1",
  "description": ":notebook: the nteract notebook",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build/ dist/ istanbuilt/",
    "lint": "eslint .",
    "pretest": "npm run coverage:instrument",
    "test": "npm run test:functional && npm run test:unit",
    "test:unit": "electron-mocha --renderer --compilers js:babel-core/register 'test/renderer/**/*.js'",
    "test:functional": "npm run build && electron-mocha --compilers js:babel-core/register 'test/main/**/*.js'",
    "test:watch": "watch 'npm run test' test/",
    "start": "electron .",
    "precoverage:instrument": "npm run build",
    "coverage:instrument": "istanbul instrument build -o istanbuilt",
    "prebuild": "rimraf build",
    "build": "babel src --out-dir build",
    "build:icon": "cd static/icons;iconutil -c icns nteract.iconset; mv nteract.icns ../icon.icns",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm run build",
    "predist": "npm run build && rimraf dist",
    "postdist": "npm install",
    "dist": "electron-packager . nteract --prune --out dist",
    "dist:linux64": "npm run dist -- --platform=linux --arch=x64",
    "dist:linux32": "npm run dist -- --platform=linux --arch=ia32",
    "dist:osx": "npm run dist -- --platform=darwin --arch=x64 --icon ./static/icon.icns --extend-info=./static/extend.plist --app-category-type=public.app-category.developer-tools --app-bundle-id=io.nteract.nteract"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nteract/nteract.git"
  },
  "keywords": [
    "jupyter",
    "electron",
    "notebook",
    "nteract",
    "data"
  ],
  "author": "nteract contributors",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/nteract/nteract/issues"
  },
  "homepage": "https://github.com/nteract/nteract",
  "dependencies": {
    "@reactivex/rxjs": "5.0.0-beta.2",
    "atom-keymap": "^6.3.1",
    "codemirror": "^5.13.4",
    "commander": "^2.9.0",
    "commutable": "^0.6.0",
    "electron-packager": "^7.0.0",
    "enchannel-zmq-backend": "^1.0.1",
    "github-api": "^0.11.2",
    "github4": "^0.5.5",
    "immutable": "^3.7.6",
    "kernelspecs": "^1.0.1",
    "normalize.css": "^4.0.0",
    "nteract-assets": "^1.0.0",
    "react": "^15.0.1",
    "react-code-mirror": "^3.0.6",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.0.1",
    "react-jupyter-display-area": "^0.3.0",
    "remark": "^4.2.0",
    "remark-react": "^2.0.0",
    "source-code-pro": "git://github.com/adobe-fonts/source-code-pro.git#release",
    "source-sans-pro": "git://github.com/adobe-fonts/source-sans-pro.git#release",
    "spawnteract": "^2.0.0",
    "transformime-react": "^0.5.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "electron-mocha": "^1.0.3",
    "electron-prebuilt": "^0.37.4",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.3.0",
    "istanbul": "^0.4.3",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "watch": "^0.17.1"
  }
}
